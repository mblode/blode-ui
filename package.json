{
  "name": "blode-ui",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npm run build:docs && npm run build:registry && next build --turbopack",
    "build:docs": "content-collections build",
    "build:registry": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check-types": "tsc --noEmit",
    "preview": "next build && next start -p 3333",
    "typecheck": "npm run build:docs && tsc --noEmit",
    "prepare": "husky",
    "check": "ultracite check",
    "fix": "ultracite fix",
    "knip": "knip",
    "knip:production": "knip --production --strict"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^16.1.6",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tabler/icons-react": "^3.36.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-form": "^1.28.3",
    "@tanstack/react-table": "^8.21.3",
    "@types/lodash.template": "^4.5.3",
    "blode-icons-react": "^0.3.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "copy-to-clipboard": "^3.3.3",
    "critters": "^0.0.25",
    "date-fns": "4.1.0",
    "downshift": "9.3.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jotai": "^2.17.1",
    "lodash": "^4.17.23",
    "lucide-react": "^0.574.0",
    "merge-refs": "^2.0.0",
    "motion": "^12.34.0",
    "next": "^16",
    "next-themes": "^0.4.6",
    "postcss": "8.5.6",
    "react": "19.2.4",
    "react-currency-input-field": "^4.0.3",
    "react-day-picker": "9.13.2",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "react-phone-number-input": "^3.4.14",
    "react-resizable-panels": "^4.6.4",
    "react-textarea-autosize": "^8.5.9",
    "react-tweet": "^3.3.0",
    "recharts": "^2.15.4",
    "remark": "^15.0.1",
    "shadcn": "3.8.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18",
    "ts-morph": "^27.0.2",
    "usehooks-ts": "^3.1.1",
    "vaul": "^1.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@content-collections/cli": "^0.1.9",
    "@content-collections/core": "^0.14.0",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/next": "^0.2.11",
    "@tailwindcss/typography": "^0.5.19",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^25.2.3",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/unist": "^3.0.3",
    "husky": "^9.1.7",
    "knip": "^5.83.1",
    "lint-staged": "^16.2.7",
    "mdast-util-toc": "^7.1.0",
    "rehype": "^13.0.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "6.0.0",
    "remark-code-import": "^1.2.0",
    "remark-gfm": "^4.0.1",
    "rimraf": "^6.1.3",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.9.3",
    "ultracite": "^7.2.3",
    "unist-builder": "^4.0.0",
    "unist-util-visit": "^5.1.0"
  },
  "overrides": {
    "date-fns": "4.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4"
  },
  "engines": {
    "node": ">=20.6.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "npx ultracite fix"
    ]
  }
